{
	"_version": "1.7.0",
	"start_url": "start.html",
	"sap.app": {
		"id": "com.sap.ovp",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{app_title}}",
		"dataSources": {
			"gc_ovp_srv": {
				"uri": "/sap/opu/odata/GC_OVP_SRV/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/gc_ovp_srv/metadata.xml"
				}
			}
		},
		"sourceTemplate": {
			"id": "OVP.smartovptemplate",
			"version": "1.41.1"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"dependencies": {
			"minUI5Version": "1.65.6",
			"libs": {
				"sap.ovp": {}
			}
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"uri": "i18n/i18n.properties"
			},
			"@i18n": {
				"preload": true,
				"type": "sap.ui.model.resource.ResourceModel",
				"uri": "i18n/i18n.properties"
			},
			"gc_ovp_srv": {
				"dataSource": "gc_ovp_srv",
				"settings": {
					"defaultCountMode": "Inline"
				}
			}
		},
		"extends": {
			"extensions": {}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		}
	},
	"sap.ovp": {
		"globalFilterModel": "gc_ovp_srv",
		"globalFilterEntityType": "",
		"containerLayout": "resizable",
		"enableLiveFilter": true,
		"considerAnalyticalParameters": false,
		"cards": {
			"card00": {
				"template": "com.sap.ovp.ext.empDetails",
				"settings": {
					"title": "{{card00_title}}"
				}
			},
			"card01": {
				"template": "com.sap.ovp.ext.approvals",
				"settings": {
					"title": "{{card01_title}}",
					"subTitle": "{{card01_subTitle}}",
					"maxrows": 15,
					"defaultSpan": {
						"rows": 20,
						"cols": 2
					}
					
				}
			}
		}
	}
}